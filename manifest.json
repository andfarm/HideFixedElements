{
    "manifest_version": 2,

    "name": "Hide Fixed Elements",
    "description": "Hide fixed elements on a page to open up more viewing space",
    "version": "1.0",

    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    },

    "browser_action": {
    },

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },

    "permissions": [
        "activeTab"
    ]
}
